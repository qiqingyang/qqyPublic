<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.apedad.example.dao.TenantMapper">

    <select id="queryTenantCount" resultType="java.lang.Integer">
        select count(1) from tenant_info
    </select>

    <select id="queryStoryCount" resultType="java.lang.Integer">
        select count(1) from ana_story
    </select>

    <select id="queryDataSetCount" resultType="java.lang.Integer">
        select count(1) from ana_dataset
    </select>
</mapper>